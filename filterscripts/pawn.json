{
	"user": "Mrucznik",
	"repo": "Texture-Studio",
	"entry": "tstudio.pwn",
	"output": "tstudio.amx",
	"dependencies": [
		"https://github.com/zeex/samp-plugin-crashdetect",
		"https://github.com/JaTochNietDan/SA-MP-FileManager",
		"https://github.com/samp-incognito/samp-streamer-plugin",
		"https://github.com/maddinat0r/sscanf",
		"Southclaws/formatex",
		"Pottus/ColAndreas"
	],
	"builds": [
		{
			"name": "release",
			"args": [
				"-;+",
				"-(+",
				"-Z+"
			],
			"includes": [
				"../pawno/include/"
			]
		},
		{
			"name": "debug",
			"args": [
				"-;+",
				"-(+",
				"-d3",
				"-Z+"
			],
			"includes": [
				"../pawno/include/"
			]
		}
	],
	"runtime": {
		"version": "0.3.7",
		"mode": "server",
		"echo": "-",
		"rcon_password": "password",
		"port": 7777,
		"hostname": "SA-MP Server",
		"maxplayers": 50,
		"language": ""
	}
}